<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.media.MediaView?>


<AnchorPane prefHeight="399.0" prefWidth="604.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.iamsinghankit.javafxlearning.MediaController">
   <children>
      <MediaView fx:id="myMedia" fitHeight="300.0" fitWidth="600.0" />
      <Button fx:id="playButton" layoutX="101.0" layoutY="332.0" mnemonicParsing="false" onAction="#playMedia" text="PLAY" />
      <Button fx:id="pauseButton" layoutX="258.0" layoutY="333.0" mnemonicParsing="false" onAction="#pauseMedia" text="PAUSE" />
      <Button fx:id="resetButton" layoutX="434.0" layoutY="334.0" mnemonicParsing="false" onAction="#resetMedia" text="RESET" />
   </children>
</AnchorPane>

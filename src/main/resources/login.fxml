<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="398.0" prefWidth="601.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.iamsinghankit.javafxlearning.LoginController">
   <children>
      <TextField fx:id="name" layoutX="252.0" layoutY="165.0" prefHeight="26.0" prefWidth="198.0" />
      <Label layoutX="74.0" layoutY="170.0" prefHeight="17.0" prefWidth="147.0" text="Username:">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Button fx:id="login" layoutX="264.0" layoutY="262.0" mnemonicParsing="false" onAction="#login" text="Login">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>

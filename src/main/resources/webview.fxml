<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.web.WebView?>

<AnchorPane prefHeight="401.0" prefWidth="601.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.iamsinghankit.javafxlearning.WebviewController">
   <children>
      <WebView fx:id="webview" layoutX="1.0" layoutY="62.0" prefHeight="339.0" prefWidth="601.0" />
      <Button fx:id="button" layoutX="1.0" layoutY="37.0" mnemonicParsing="false" onAction="#loadPage" text="Load" />
      <TextField fx:id="textfield" layoutX="1.0" layoutY="12.0" onAction="#loadPage" prefHeight="26.0" prefWidth="601.0" />
      <Button layoutX="56.0" layoutY="38.0" mnemonicParsing="false" onAction="#refreshPage" text="Refresh" />
      <Button layoutX="129.0" layoutY="39.0" mnemonicParsing="false" onAction="#zoomIn" text="+" />
      <Button layoutX="167.0" layoutY="38.0" mnemonicParsing="false" onAction="#zoomOut" text="_" />
      <Button layoutX="200.0" layoutY="37.0" mnemonicParsing="false" onAction="#displayHistory" text="History" />
      <Button layoutX="265.0" layoutY="38.0" mnemonicParsing="false" onAction="#back" text="&lt;" />
      <Button layoutX="301.0" layoutY="38.0" mnemonicParsing="false" onAction="#forward" text="&gt;" />
      <Button layoutX="333.0" layoutY="39.0" mnemonicParsing="false" onAction="#executeJS" text="executeJS" />
   </children>
</AnchorPane>

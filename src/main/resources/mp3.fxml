<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pane" prefHeight="170.0" prefWidth="663.0" style="-fx-background-color: #222222;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.iamsinghankit.javafxlearning.Mp3Controller">
   <Label fx:id="songLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="-4.0" prefHeight="107.0" prefWidth="667.0" text="MP3" textFill="#0b8a8c">
      <font>
         <Font size="51.0" />
      </font>
   </Label>
   <ProgressBar fx:id="songProgressBar" layoutX="2.0" layoutY="110.0" prefHeight="20.0" prefWidth="656.0" progress="0.0" />
   <Button fx:id="playButton" layoutY="130.0" mnemonicParsing="false" onAction="#play" text="PLAY">
      <font>
         <Font size="18.0" />
      </font>
   </Button>
   <Button fx:id="pauseButton" layoutX="67.0" layoutY="130.0" mnemonicParsing="false" onAction="#pause" text="PAUSE">
      <font>
         <Font size="18.0" />
      </font>
   </Button>
   <Button fx:id="resetButton" layoutX="146.0" layoutY="130.0" mnemonicParsing="false" onAction="#reset" text="RESET">
      <font>
         <Font size="18.0" />
      </font>
   </Button>
   <Button fx:id="previousButton" layoutX="222.0" layoutY="130.0" mnemonicParsing="false" onAction="#previous" text="PREVIOUS">
      <font>
         <Font size="18.0" />
      </font>
   </Button>
   <Button fx:id="nextButton" layoutX="329.0" layoutY="130.0" mnemonicParsing="false" onAction="#next" text="NEXT">
      <font>
         <Font size="18.0" />
      </font>
   </Button>
   <ComboBox fx:id="speedComboBox" layoutX="398.0" layoutY="130.0" onAction="#changeSpeed" prefHeight="35.0" prefWidth="93.0" promptText="SPEED" />
   <Slider fx:id="volumeSlider" layoutX="491.0" layoutY="141.0" prefHeight="16.0" prefWidth="170.0" value="50.0" />
</AnchorPane>
